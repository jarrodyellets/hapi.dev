{
  "name": "hapi.dev",
  "version": "1.0.0",
  "description": "The hapi.dev developer portal",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "gen": "nuxt generate",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "node assets/js/getModuleInfo && nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "devTest": "node assets/js/getModuleInfo",
    "genTS": "typedoc static/lib/typeIndexes --out static/type-docs/ --mode modules --stripInternal --readme none --target ES6 --plugin typedoc-plugin-markdown --ignoreCompilerErrors --includeDeclarations --disableOutputCheck --excludeExternals -hideSources -hideBreadcrumbs",
    "testDocs": "node assets/js/getModuleInfo & typedoc static/lib/typeIndexes --out static/type-docs/ --mode modules --stripInternal --readme none --target ES6 --plugin typedoc-plugin-markdown --ignoreCompilerErrors --includeDeclarations --disableOutputCheck --excludeExternals -hideSources -hideBreadcrumbs",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@hapi/joi": "17.x.x",
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/markdownit": "^1.2.4",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/svg": "^0.1.6",
    "@types/node": "^13.9.3",
    "axios": "^0.19.0",
    "bulma": "^0.7.4",
    "cookie-universal-nuxt": "^2.0.17",
    "cross-env": "^5.2.0",
    "dom-parser": "^0.1.6",
    "fs": "0.0.1-security",
    "hapi-nuxt": "^1.0.1",
    "highlight.js": "^9.18.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "markdown-toc": "^1.2.0",
    "nuxt": "^2.4.0",
    "rimraf": "^3.0.2",
    "semver": "^6.0.0",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^3.8.3",
    "vue-codemirror": "^4.0.6",
    "vue-highlightjs": "^1.3.3",
    "vue-js-modal": "^1.3.31"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "sass-loader": "^7.3.1",
    "typedoc": "^0.17.3"
  }
}
