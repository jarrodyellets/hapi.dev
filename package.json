{
  "name": "hapi.dev",
  "version": "1.0.0",
  "description": "The hapi.dev developer portal",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "testDocs": "node assets/js/genTypeDocs & typedoc static/lib/typeIndexes --out static/type-docs/ --mode modules --readme none --target ES6 --plugin typedoc-plugin-markdown --ignoreCompilerErrors --includeDeclarations --disableOutputCheck --excludeExternals -hideSources -hideBreadcrumbs & nuxt generate",
    "generateDocs": "typedoc static/lib/typeIndexes --out static/type-docs/ --mode file --readme none --target ES6 --plugin typedoc-plugin-markdown --ignoreCompilerErrors --includeDeclarations --disableOutputCheck --excludeExternals -hideSources -hideBreadcrumbs"
  },
  "dependencies": {
    "@hapi/wreck": "^16.0.1",
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/markdownit": "^1.2.4",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/svg": "^0.1.6",
    "@types/node": "^12.12.5",
    "axios": "^0.19.0",
    "bulma": "^0.7.4",
    "cookie-universal-nuxt": "^2.0.17",
    "cross-env": "^5.2.0",
    "dom-parser": "^0.1.6",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "hapi-nuxt": "^1.0.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "markdown-toc": "^1.2.0",
    "nuxt": "^2.4.0",
    "semver": "^6.0.0",
    "typedoc-plugin-markdown": "^2.2.11",
    "vue-js-modal": "^1.3.31"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.17.0",
    "sass-loader": "^7.1.0",
    "typedoc": "^0.15.0"
  }
}
